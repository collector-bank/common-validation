<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <Company>Collector Bank AB</Company>
    <Product>National Identifier Validator</Product>
    <Description>Tools to validate and normalise national identifiers (social security numbers) of Denmark, Finland, Norway and Sweden.</Description>
    <PackageLicenseUrl>https://github.com/collector-bank/common-validation/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/collector-bank/common-validation</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>validation, normalization, social security number, national identifier, personal number, sweden, denmark, finland, norway</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIconUrl>https://raw.githubusercontent.com/collector-bank/common-correlation/master/icon.png</PackageIconUrl>
    <AssemblyVersion>2.0.0.0</AssemblyVersion>
    <FileVersion>2.0.0.0</FileVersion>
    <Version>2.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.ComponentModel.Annotations" Version="5.0.0" />
  </ItemGroup>

</Project>
